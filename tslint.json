{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
      "no-console": false,
      "no-shadowed-variable": false,
      "comment-format": false,
      "no-trailing-whitespace": false,
      "no-var-requires": false,
      "no-empty": false,
      "no-unused-expression": false,
      "prefer-const": false,
      "prefer-for-of": false
    },
    "rulesDirectory": []
}
  